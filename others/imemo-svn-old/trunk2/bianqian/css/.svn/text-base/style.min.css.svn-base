/* CSSTidy 1.3: Wed, 30 Dec 2009 12:55:26 +0800 */
body,ul,ol,li,p,h1,h2,h3,h4,h5,h6,form,fieldset,table,td,img,div{border:0;margin:0;padding:0;}
body{background:#fff;color:#888;font-family:verdana,arial,helvetica,clean,sans-serif;font-size:12px;}
ul,ol{list-style-type:none;}
select,input,img,select{vertical-align:middle;}
table{border-collapse:collapse;}
a{text-decoration:none;}
h1,h2,h3,h4,h5,h6{font-weight:700;}
h1{font-size:17px;}
h2{font-size:16px;}
h3{font-size:15px;}
h4{font-size:14px;}
h5{font-size:13px;}
p{line-height:1.4em;}
.clearfix:after{clear:both;content:".";display:block;height:0;visibility:hidden;}
************ html .clearfix{height:1%;}
************+html .clearfix{min-height:1%;}
.ui-resizable-handle{z-index:auto;}
.ui-lds-drag{opacity:0.8;}
.ui-lds-icon{cursor:pointer;display:block;height:17px;width:17px;}
.ui-button{cursor:pointer;text-align:center;}
.ui-dialog .ui-state-highlight,.ui-dialog .ui-state-error{padding:.3em;}
.sp6{background-image:url(../images/google-tasks-sprites6.png);background-repeat:no-repeat;}
#wrap{margin:10px auto;}
#header{height:50px;line-height:50px;margin-top:20px;background:url(../images/nav_bg.gif) repeat-x left bottom;}
#toolbar{height:30px;margin-top:8px;}
#content{margin-top:20px;}
#main{min-height:400px;_height:400px;border:none;padding:0;}
.sidebar{float:left;width:19%;}
#categorys{width:15%;background:none;border:none;padding:0;}
#innerContent{float:right;margin-right:3px;width:80%;}
#footer{border-top:1px #ddd dashed;margin:10px 0 0;padding:5px 0;}
#default #content{margin-left:0;}
#logo{float:left;margin:0 2px 0 0;}
#nav li{float:left;list-style-type:none;}
#nav li a{float:left;background:#fff;display:block;height:47px;line-height:47px;font-weight:700;padding:0 10px;}
#nav li a.now{background:url(../images/nav_now.gif) no-repeat center bottom;}
#search_text{border:1px #ccc solid;padding-left:2px;}
#search_submit{background:url(../images/icon_search_.gif) no-repeat 0 0;cursor:pointer;height:26px;width:31px;margin:0 0 0 5px;padding:0;}
#search_submit.ui-state-hover{background-position:0 -41px;border:none;}
#intro{float:left;text-align:right;width:240px;white-space:nowrap;}
#message{background:orange url(../images/msg_bg.gif) no-repeat right center;color:#fff;display:none;float:left;font-weight:700;height:21px;line-height:21px;margin-left:200px;padding:0 20px 0 0;}
#message span{background:url(../images/msg_bg.gif) no-repeat left center;display:block;padding:0 0 0 20px;}
#message a{color:#0F8BC9;text-decoration:underline;}
#message a:hover{color:#000;}
#user{float:right;_width:350px;white-space:nowrap;}
#user li{float:right;margin:0 5px 0 0;}
#form_login{background:#fff;border:1px #ddd solid;padding:20px;}
#form_login input.text{margin-bottom:12px;width:255px;padding:.4em;}
#form_login .error{background:#FFE9AF;border:1px dashed #ddd;padding:5px;}
.from_error{color:red;margin:0 5px;}
#my-profile{border:1px #ddd dashed;padding:10px;}
#categorys li{background:none;border:none;margin:0 0 1px;padding:0;}
#categorys li a{background:#A9C1C9;color:#fff;display:block;font-family:Courier;font-weight:700;height:35px;line-height:35px;width:100px;margin:0;padding:0 0 0 30px;}
#categorys li a:hover,#categorys li.ui-tabs-selected a{color:#000;padding-left:50px;}
#categorys li a.now{background:#7C8A8C;}
#innerContent .ui-tabs-panel{padding:0;}
#innerContent .cate{margin:0;padding:0;}
.note{background:#f9f9f9 url(../images/msg_bg.gif) repeat 0 0;border:1px #ddd solid;float:left;margin-bottom:5px;position:relative;width:100%;}
#innerContent #note_00{background-color:#fff;margin-bottom:10px;}
#note_00 .n_content{width:87%;}
.ajax-add-note{background:none;border:none;border-bottom:1px #ddd dashed;width:95%;}
.n_col{float:left;height:17px;line-height:17px;padding:10px 0;}
.n_content{background:transparent;}
.n_lable{background:#fcfcfc;border-right:1px #ddd solid;cursor:move;width:10px;}
.n_lable.hover,.n_lable:hover{background:url(../images/google-tasks-sprites6.png) no-repeat -252px 0;}
.n_date,.n_time,.n_icon_date{color:#888;float:right;font-size:11px;margin-right:10px;margin-top:8px;padding:2px;}
.n_input{background:none;height:17px;border:none;text-align:left;cursor:pointer;}
input.n_date,input.n_time{padding-left:20px;}
#main .c_min{width:20px;padding-left:0;}
input.n_date{background:transparent url(../images/icon_calendar.gif) no-repeat left center;width:67px;margin-right:5px;}
input.n_time{background:transparent url(../images/icon_clock.png) no-repeat left center;width:35px;}
.n_del{float:right;width:28px;}
.n_tag{font-size:11px;opacity:0.7;position:absolute;right:150px;top:12px;display:none;}
.n_tag span{background:#E0ECFF;color:#5391FF;margin:0 0 0 2px;padding:2px;}
.n_state{margin:0 5px;}
.checkbox{background:#fff url(../images/google-tasks-sprites6.png) repeat -16px -2px;cursor:pointer;height:16px;width:16px;}
.done .checkbox{background-color:transparent;background-position:-32px -2px;}
.note table{border-collapse:collapse;border-spacing:0;empty-cells:show;width:100%;}
.n_content,.n_time{cursor:text;outline-style:none;outline-width:medium;}
.n_time{cursor:pointer;}
.done .n_content{color:gray;text-decoration:line-through;}
li.extend .n_content,li.extend .n_date{background:#eee;display:block;float:none;width:100%;}
li.extend .n_content .hidden,li.extend .n_date .hidden{display:inline;width:50px;}
#editContent{width:100%;}
#main.welcome-page{text-align:center;}
.border-radius{height:1px;}
.border_00{background:#fff;padding:5px;}
.submit_1{border:none;}
.grepinput{color:#ccc;}
.title_1{background:url(../images/title_1_bg.gif) no-repeat 0 0;color:#0B6B93;font-family:Courier;height:46px;line-height:46px;margin-bottom:10px;padding-left:30px;}
.loading{background-image:url(../images/icon_loading2.gif);background-position:90% center;background-repeat:no-repeat;height:20px;width:20px;}
.categorys_list{border:1px #eee solid;margin-top:10px;padding:10px;}
.categorys_list tr{height:30px;}
.categorys_list .even{background-color:#eee;}
.categorys_list .c_ckbox,.categorys_list .c_del,.categorys_list .c_rename{text-align:center;width:30px;}
.categorys_list .c_name{width:250px;}
#goto_register{text-align:right;}
#js_goto_register{font-size:16px;margin:0 5px 0 0;}
#js_goto_register:hover{background:#2193CD;color:#fff;}
div.time-picker{position:absolute;height:200px;#width:4em;width:53px;overflow:auto;background:#fff;border:1px solid #000;z-index:99;margin-top:20px;margin-left:15px;padding:2px;}
div.time-picker-12hours{width:6em;}
div.time-picker ul{list-style-type:none;margin:0;padding:0;}
div.time-picker li{cursor:pointer;padding:1px;}
div.time-picker li.selected{background:#316AC5;color:#fff;}
a:link,a:visited{color:#0F8BC9;}
a:hover,a:active,a:focus,.n_input:focus,.n_content:focus{color:#66CBFF;}
h6,.ui-widget{font-size:12px;}
.clearfix,#form_login label,#form_login input{display:block;}
#footer p.coryright,#nav{float:left;}
#footer ul.links,#search,#create_category_form{float:right;}
#footer .links li,#form_login #ajax-fetch-captcha{display:inline;}
#header,#content,#toolbar,#footer,#innerContent #note_00 .n_content{margin-left:10px;margin-right:10px;}
.extend .n_col,.hidden,.categorys_list .c_id{display:none;}
.top1,.bottom1{background:#fff;margin:0 1px;}
.top2,.bottom2{background:#fff;margin:0 2px;}
